<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% cards.forEach(card => { %>
      <% if(card.pickOrder && card.imageUrl) { %>
        <img class="card-img" src="<%= card.imageUrl %>" alt="<%= card.pickOrder %>">
      <% } %>
    <% }); %>

  </body>
</html>
